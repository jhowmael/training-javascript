  "bugs": {
    "url": "https://github.com/jhowmael/training-javascript/issues"
  },
  "homepage": "https://github.com/jhowmael/training-javascript#readme",
  "devDependencies": {
    "@types/node": "^22.15.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}