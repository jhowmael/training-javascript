{
  "name": "training-javascript",
  "version": "1.0.0",
  "type": "module",
  "description": "A repository focused on logic exercises using javascript",
  "main": "index.js",
  "scripts": {
    "run:procedure": "for %f in (procedure\\*.ts) do node --no-warnings --loader ts-node/esm %f",
    "run:poo": "for %f in (poo\\*.ts) do node --no-warnings --loader ts-node/esm %f",
    "run:array": "for %f in (arrays_tests\\*.ts) do node --no-warnings --loader ts-node/esm %f",
    "run:curse": "for %f in (udemy_curse\\*.ts) do node --no-warnings --loader ts-node/esm %f",
    "run:all": "npm run run:procedure && npm run run:poo && npm run run:array && npm run run:curse"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhowmael/training-javascript.git"
  },
